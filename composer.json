{
    "name": "peppe/laravel-route-translator",
    "description": "A package to translate Laravel routes",
    "keywords": ["laravel", "route", "translator"],
    "license": "MIT",
    "authors": [
        {
            "name": "Peppe",
            "email": "peppe.vignanello@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/contracts": "^10.0|^11.0|^12.0"
    },
    "autoload": {
        "psr-4": {
            "TuoNome\\RouteTranslator\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "TuoNome\\RouteTranslator\\RouteTranslatorServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
